{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"name": "example",
	"projectType": "application",
	"sourceRoot": "{projectRoot}/src",
	"targets": {
		"check": {},
		"serve": {
			"command": "bun --inspect src/index.ts",
			"options": {
				"cwd": "{projectRoot}"
			}
		},
		"send": {
			"command": "bun --inspect src/client.ts",
			"options": {
				"cwd": "{projectRoot}"
			}
		}
	}
}
